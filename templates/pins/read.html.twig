{% extends 'base.html.twig' %}

{% block title %}{{ pin.title }}{% endblock %}

{% block body %}

    <article>
        <h1>Pin {{ pin.id }}</h1>
        <p>{{ pin.description }}</p>
    </article>
    
    <a href="{{ path('app_pin_edit', {id: pin.id} ) }}">Editer</a>
     | 
    <a href="{{ path('app_home') }}">Retour</a>

{% endblock %}